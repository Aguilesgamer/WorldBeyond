{
	"format_version": "1.16.0",
	"minecraft:entity": {
		"description": {
			"identifier": "minecraft:player",
			"is_spawnable": false,
			"is_summonable": false,
			"is_experimental": false,
			"scripts": {
				"animate": [
					// Advancements
					"advancements",
					"advancement.feet_wet",
					"advancement.adventuring_journey",
					// Equipment
					"solar_medallion",
					// Block Fixes
					"flag_items"
				]
			},
			"animations": {
				"advancements": "controller.animation.server.player_advancements.general",
				"advancement.feet_wet": "controller.animation.server.player_advancements.feet_wet",
				"advancement.adventuring_journey": "controller.animation.server.player_advancements.adventuring_journey",
				"solar_medallion": "controller.animation.solar_medallion",
				"flag_items": "controller.animation.flag_items"
			}
		},
		"component_groups": {
			// Advancements
			"worldbeyond:advancement.general": {
				"minecraft:mark_variant": {
					"value": 0
				}
			},
			"worldbeyond:advancement.feet_wet": {
				"minecraft:mark_variant": {
					"value": 1
				}
			},
			"worldbeyond:advancement.adventuring_journey": {
				"minecraft:mark_variant": {
					"value": 2
				}
			},
			// Sneaking
			"worldbeyond:sneaking": {
				"minecraft:collision_box": {
					"width": 0.6,
					"height": 1.5
				}
			},
			"worldbeyond:vanilla_hitbox": {
				"minecraft:collision_box": {
					"width": 0.6,
					"height": 1.8
				}
			},
			// Defaults
			"minecraft:add_bad_omen": {
				"minecraft:spell_effects": {
					"add_effects": [
						{
							"effect": "bad_omen",
							"duration": 6000,
							"visible": false,
							"display_on_screen_animation": true
						}
					]
				},
				"minecraft:timer": {
					"time": [
						0.0,
						0.0
					],
					"looping": false,
					"time_down_event": {
						"event": "minecraft:clear_add_bad_omen",
						"target": "self"
					}
				}
			},
			"minecraft:clear_bad_omen_spell_effect": {
				"minecraft:spell_effects": {}
			},
			"minecraft:raid_trigger": {
				"minecraft:raid_trigger": {
					"triggered_event": {
						"event": "minecraft:remove_raid_trigger",
						"target": "self"
					}
				},
				"minecraft:spell_effects": {
					"remove_effects": "bad_omen"
				}
			}
		},
		"components": {
			"minecraft:experience_reward": {
				"on_death": "Math.Min(query.player_level * 7, 100)"
			},
			"minecraft:type_family": {
				"family": [
					"player",
					"human"
				]
			},
			"minecraft:is_hidden_when_invisible": {},
			"minecraft:loot": {
				"table": "loot_tables/empty.json"
			},
			"minecraft:collision_box": {
				"width": 0.6,
				"height": 1.8
			},
			"minecraft:can_climb": {},
			"minecraft:movement": {
				"value": 0.1
			},
			"minecraft:hurt_on_condition": {
				"damage_conditions": [
					{
						"filters": {
							"test": "in_lava",
							"subject": "self",
							"operator": "==",
							"value": true
						},
						"cause": "lava",
						"damage_per_tick": 4
					}
				]
			},
			"minecraft:attack": {
				"damage": 1
			},
			"minecraft:player.saturation": {
				"value": 20
			},
			"minecraft:player.exhaustion": {
				"value": 0,
				"max": 4
			},
			"minecraft:player.level": {
				"value": 0,
				"max": 24791
			},
			"minecraft:player.experience": {
				"value": 0,
				"max": 1
			},
			"minecraft:breathable": {
				"total_supply": 15,
				"suffocate_time": -1,
				"inhale_time": 3.75,
				"generates_bubbles": false
			},
			"minecraft:nameable": {
				"always_show": true,
				"allow_name_tag_renaming": false
			},
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": false,
				"is_pushable_by_piston": true
			},
			"minecraft:insomnia": {
				"days_until_insomnia": 3
			},
			"minecraft:rideable": {
				"seat_count": 2,
				"family_types": [
					"parrot_tame"
				],
				"pull_in_entities": true,
				"seats": [
					{
						"position": [
							0.4,
							-0.2,
							-0.1
						],
						"min_rider_count": 0,
						"max_rider_count": 0,
						"lock_rider_rotation": 0
					},
					{
						"position": [
							-0.4,
							-0.2,
							-0.1
						],
						"min_rider_count": 1,
						"max_rider_count": 2,
						"lock_rider_rotation": 0
					}
				]
			},
			"minecraft:conditional_bandwidth_optimization": {},
			"minecraft:block_climber": {},
			"minecraft:environment_sensor": {
				"triggers": [
					{
						"filters": {
							"all_of": [
								{
									"test": "has_mob_effect",
									"subject": "self",
									"value": "bad_omen"
								},
								{
									"test": "is_in_village",
									"subject": "self",
									"value": true
								}
							]
						},
						"event": "minecraft:trigger_raid"
					},
					{
						"filters": {
							"all_of": [
								{
									"test": "is_sneaking",
									"subject": "self",
									"value": true
								}
							]
						},
						"event": "worldbeyond:sneak"
					},
					{
						"filters": {
							"all_of": [
								{
									"test": "is_sneaking",
									"subject": "self",
									"value": false
								}
							]
						},
						"event": "worldbeyond:unsneak"
					}
				]
			}
		},
		"events": {
			// Advancements
			"player:advancement.clear_cooldown": {
				"add": {
					"component_groups": [
						"worldbeyond:advancement.general"
					]
				}
			},
			"player:advancement_get.feet_wet": {
				"add": {
					"component_groups": [
						"worldbeyond:advancement.feet_wet"
					]
				}
			},
			"player:advancement_get.adventuring_journey": {
				"add": {
					"component_groups": [
						"worldbeyond:advancement.adventuring_journey"
					]
				}
			},
			//Sneaks
			"worldbeyond:sneak": {
				"add": {
					"component_groups": [
						"worldbeyond:sneaking"
					]
				}
			},
			"worldbeyond:unsneak": {
				"add": {
					"component_groups": [
						"worldbeyond:vanilla_hitbox"
					]
				}
			},
			// Defaults
			"minecraft:gain_bad_omen": {
				"add": {
					"component_groups": [
						"minecraft:add_bad_omen"
					]
				}
			},
			"minecraft:clear_add_bad_omen": {
				"remove": {
					"component_groups": [
						"minecraft:add_bad_omen"
					]
				},
				"add": {
					"component_groups": [
						"minecraft:clear_bad_omen_spell_effect"
					]
				}
			},
			"minecraft:trigger_raid": {
				"add": {
					"component_groups": [
						"minecraft:raid_trigger"
					]
				}
			},
			"minecraft:remove_raid_trigger": {
				"remove": {
					"component_groups": [
						"minecraft:raid_trigger"
					]
				}
			}
		}
	}
}